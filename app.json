{
  "expo": {
    "name": "Smart Home",
    "slug": "SmartHome",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],

    "android": {
      "package": "com.SmartReact.SmartHome"
    },
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/smarthome.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}